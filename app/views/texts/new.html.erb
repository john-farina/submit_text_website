<%= link_to image_tag("home.png", class: "homeIcon"), root_path, class: "iconContainer" %>
<%= form_with(model: @text, local: true, class: "formContainer") do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :text %>
  <%= f.text_field :text %>
  <%= f.submit "Submit Text" %>
<% end %>
